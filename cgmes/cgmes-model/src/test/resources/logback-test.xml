<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-5p %d{HH:mm:ss.SSS} %-20C{1} | %m%n</pattern>
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>
    <logger name="com.bigdata" level="warn" additivity="false">
        <appender-ref ref="STDOUT" />
    </logger>
</configuration>
